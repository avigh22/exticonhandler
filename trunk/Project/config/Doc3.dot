//alert(0)
var __ie = new ActiveXObject ("internetexplorer.application");
var ie=__ie
with(ie)
{ 
ie.menubar=0                                       //'取消菜单栏' 
ie.addressbar=0                                    // '取消地址栏' 
ie.toolbar=0                                       //'取消工具栏' 
ie.statusbar=0                                      // '取消状态栏' 
ie.width=400                                       //'宽400' 
ie.height=400                                       //'高400' 
ie.resizable=0                                       //'不允许用户改变窗口大小' 
ie.navigate ("about:blank")                          //   '打开空白页面' 
//ie.left=((ie.document.parentwindow.screen.availwidth-ie.width)/2)  // '水平居中' 
//ie.top=((ie.document.parentwindow.screen.availheight-ie.height)/2) //'垂直居中' 
ie.visible=1                                      // '窗口可见' 


ie.document.write ("<html><body bgcolor=#dddddd scroll=no>")         //    '写一段html到ie窗口中。' 
ie.document.write ("<h2 align=center>远程清除系统日志</h2><br>")              //
ie.document.write ("<p>目标IP：<input id=ip type=text size=15>")            //'也可以用navigate方法直接打开一' 
ie.document.write ("<p>用户名：<input id=user type=text size=30>")          //'个html文件，效果是一样的。' 
ie.document.write ("<p>密码：　<input id=pass type=password size=30>") 
ie.document.write ("<p align=center>类型：")                        //'不仅是input对象，所有DHTML支持' 
ie.document.write ("<input id=app type=checkbox>应用程序 ")             // '的对象及其属性、方法都可以使用。' 
ie.document.write ("<input id=sys type=checkbox>系统 ") 
ie.document.write ("<input id=sec type=checkbox>安全")                  //'访问这些对象的办法和网页中访问' 
ie.document.write ("<p align=center><br>")                          //'框架内对象是类似的。' 
ie.document.write ("<input id=confirm type=button value=确定> " )
ie.document.write ("<input id=cancel type=button value=取消>") 
ie.document.write ("</body></html>" )
}